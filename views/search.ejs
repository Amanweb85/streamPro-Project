<div class="page-manager">
  <!---------------------  left panel ------------------>
  <div class="left-panel-container">
    <div class="left-panel">
      <div class="shortcuts">
        <span class="shortcut-GroupLabel">Discover</span>
        <a href="/" class="active flex-box">
          <img alt="" src="/dist/images/home.png" />
          <p>Home</p>
        </a>
        <a id="subscription" class="flex-box">
          <img alt="" src="/dist/images/subscription.png" />
          <p>Subscription</p>
        </a>
        <a href="/search?sq=trending" class=" flex-box">
          <img alt="" src="/dist/images/explore.png" />
          <p>Trending</p>
        </a>
        <!-- <a class="flex-box">
          <img alt="" src="/dist/images/cast.png" />
          <p>Continue Watching</p>
        </a> -->
        <hr>
        <span class="shortcut-GroupLabel">Your Library</span>
        <a id="library" class="flex-box">
          <img alt="" src="/dist/images/library.png" />
          <p>Library</p>
        </a>
        <a id="liked-videos" class="flex-box">
          <img alt="" src="/dist/images/like.png" />
          <p>Liked Videos</p>
        </a>
        <a href="/search/user/history" id="history" class="flex-box">
          <img alt="" src="/dist/images/history.png" />
          <p>History</p>
        </a>
        <a id="playlists" class="flex-box">
          <img alt="" src="/dist/images/playlist.png" />
          <p>Playlists</p>
        </a>
        <a href="/search/user/favourites" id="playlists" class="flex-box">
          <img alt="" src="/dist/images/playlist.png" />
          <p>Favorites</p>
        </a>
        <hr>
        <span class="shortcut-GroupLabel">Browse</span>
        <a id="playlists" class="flex-box">
          <img alt="" src="/dist/images/playlist.png" />
          <p>Categories</p>
        </a>
      </div>
    </div>
  </div>
  <!-- --------------------main--------------------- -->

  <div class="main flex-box">

    <div id="top" class="banner">
      <div class="banner-overlay">
        <div class="remove-banner">&#x2716;</div>
        <h1>Welcome to the
          <p>streamPro</p>
        </h1>
        <h2>Enjoy Your Favourite <span></span></h2>
        <p class="hero-descriotion">Join the journey through uncharted worlds as one explorer discovers the secrets of a distant civilization. A groundbreaking sci-fi epic.</p>
        <div style="
    gap: 1rem;
    font-weight: bold;
    display: flex;
"><button class="btn1">
            <!-- <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" style="width: 18px;
    color: black;
    fill: currentColor;
    font-size: 14px;
">
              <polygon points="6 3 20 12 6 21 6 3"></polygon>
            </svg> -->
            <img style="
    gap: 1rem;
    font-weight: bold;
    display: flex;
    height: 1rem;
" src="\static\images\play.png" alt="">
            Watch Now
          </button>
          <button class="btn2"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" style="width: 18px;">
              <circle cx="12" cy="12" r="10"></circle>
              <path d="M12 16v-4"></path>
              <path d="M12 8h.01"></path>
            </svg>More Info</button>
        </div>
      </div>
    </div>
    <div class="vid-category">
      <div id="div1">All</div>
      <!-- Categories Section -->

      <!-- <div class="categoryId-27" id="27">Education</div> -->
      <div class="categoryId-10" id="10">Music</div>
      <div class="categoryId-17" id="17">Sports</div>
      <div class="categoryId-20" id="20">Gaming</div>
      <div class="categoryId-23" id="23">Comedy</div>
      <div class="categoryId-24" id="24">Entertainment</div>
      <div class="categoryId-25" id="25">News & Politics</div>
      <div class="categoryId-1" id="1">Film & Animation</div>
      <div class="categoryId-15" id="15">Pets & Animals</div>
      <div class="categoryId-28" id="28">Science & Technology</div>
      <div class="categoryId-2" id="2">Autos & Vehicles</div>
      <div class="categoryId-22" id="22">People & Blogs</div>
      <div class="categoryId-26" id="26">Howto & Style</div>
      <div class="categoryId-29" id="29">Nonprofits & Activism</div>

    </div>
    <div class="video-area">


      <% if (searchVideos.items ) { %>
      <script>
        window.__APP_DATA__ = <%- JSON.stringify(searchVideos)%>;
      </script>
      <% searchVideos.items.forEach(item=> { %>
      <% if (item.type==='channel' )
      { %>
      <%- include('partials/search/channel-card', { item }) %>
      <% } else if
      (item.type==='playlist'  ) {
      %>
      <%- include('partials/search/playlist-card', { item }) %>
      <% } else {
      %>
      <%- include('partials/search/video-card', { item }) %>
      <% } %>
      <% }) %>
      <% }%>
    </div>
  </div>
</div>